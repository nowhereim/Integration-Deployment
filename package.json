    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --config=jest.config.js --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm",
    "migration:generate": "npm run build && typeorm migration:generate src/migrations/migrations -d dist/src/config/ormconfig.js",
    "migration:run": "npm run build && typeorm migration:run -d dist/src/config/ormconfig.js",
    "migration:revert": "npm run build && typeorm migration:revert -d dist/config/ormconfig.js"
  },
